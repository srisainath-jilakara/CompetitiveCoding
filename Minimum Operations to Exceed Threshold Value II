class Solution {
    public int minOperations(int[] nums, int k) {
        PriorityQueue<Long> pq = new PriorityQueue<>();
        for(int num: nums) {
            pq.offer((long)num);
        }
        int count=0;
        while(pq.size()>=2 && pq.peek()<k) {
            long x = pq.poll();
            long y = pq.poll();
            long ele = x *2 + y;
            pq.offer(ele);
            count++;
        }
        return pq.peek()>=k?count:-1;
    }
}
